{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120,
        "tabWidth": 2
      }
    ],
    "no-alert": 0,
    "no-restricted-globals": 0,
    "no-unused-vars": 0,
    "no-plusplus": 0,
    "no-fallthrough": 0,
    "prefer-template": 0
  },
  "env": {
    "browser": true
  }
}
